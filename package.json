{
  "name": "primape-markets-platform",
  "version": "1.0.0",
  "description": "Solana Prediction Markets Platform",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd src/frontend && npm install",
    "dev": "./run-dev.sh",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd src/frontend && npm run dev",
    "test:believe": "node scripts/test-believe-api-burn.js",
    "build:frontend": "cd src/frontend && npm run build",
    "build:contracts": "cd src/smart_contracts/market_system && anchor build",
    "test:contracts": "cd src/smart_contracts/market_system && anchor test"
  },
  "keywords": [
    "solana",
    "prediction-markets",
    "blockchain",
    "defi"
  ],
  "author": "PRIMAPE",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.3",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.0"
  }
}
